        <FunctionBlock UUID="F185FE7C49EE1A6AF29879ABE1E5FF85" Name="AI_Proc_Mal">
            <Variables>
                <Variable UUID="CB2A7B4A4C204B90164842A18335F3D0" Name="SEN_AI" Type="support_AI" TypeUUID="A32E304D43358DC1574ABBA8FDAFBC2D" Usage="inout" />
                <Variable UUID="4CB9B2A84D0F937D88923D9905B9BB10" Name="AI_raw" Type="UINT" TypeUUID="C50B45D04CDCD1FA8859FAB8CB6FC7EE" Usage="input" />
                <Variable UUID="0ABAF0FA4FE01DB8D3ACB891A69785B0" Name="cycleTime" Type="LREAL" TypeUUID="65F1DDD44EDA9C0776BB16BBDFE36B1F" Usage="input" />
                <Variable UUID="CBCA772747FDCE2374A7AABB441CE4A1" Name="AI_HMI_str" Type="AI_HMI" TypeUUID="A66BBC2040DE5F1DB91157BC6ACE32E7" Usage="inout" />
                <Variable UUID="429D84EC498F139E0A9871B89A5FDCF3" Name="AI_C_BLK" Type="proc_AI" TypeUUID="E0D63CC74D1606235F84069D885963D7" Usage="internal" />
            </Variables>
            <ST><![CDATA[//Конфигурация. Изменения со СКАДЫ (не используется!!!!!!!!!!!!!!)

(*SEN_AI.Config.Span := AI_HMI_str.Cfg_Span;
SEN_AI.Config.Offset := AI_HMI_str.Cfg_Offset;
SEN_AI.Config.min_fault_sensor_Eu := SEN_AI.Config.Offset-SEN_AI.Config.Span*LREAL#0.03125;
SEN_AI.Config.max_fault_sensor_Eu := SEN_AI.Config.Span+SEN_AI.Config.Offset+SEN_AI.Config.Span*LREAL#0.012;
SEN_AI.Config.repair_time := AI_HMI_str.Cfg_repair_time;
SEN_AI.Config.min_ADC := AI_HMI_str.Cfg_min_ADC;
SEN_AI.Config.max_ADC := AI_HMI_str.Cfg_max_ADC;
SEN_AI.Config.ROC := AI_HMI_str.Cfg_ROC;*)

//-----------------------------------------------------------------//
// Запись сырого значения в структуру
SEN_AI.Oper.In := UINT_TO_LREAL(AI_raw);

// Запись в структуру признака ремонта канала
SEN_AI.Config.Manual_Auto.0 := AI_HMI_str.Sim;

// Запись в структуру задания канала в ручном режиме
SEN_AI.Config.Manual_Target := AI_HMI_str.Manual;

// Запись значения Eu_user в структуру
SEN_AI.Config.Eu_user := AI_HMI_str.Eu;

// Запись значения ROC в структуру
SEN_AI.Config.ROC := AI_HMI_str.ROC;

// Запись значения времени ремонта в структуру
SEN_AI.Config.repair_time := AI_HMI_str.TM_Repair;
//-----------------------------------------------------------------//


//-----------------------------------------------------------------//
// Вызов C-блока
AI_C_BLK(SEN_AI, cycleTime);

// Если блок вернул канал в автомат
AI_HMI_str.Sim := SEN_AI.Config.Manual_Auto.0;

// Обработанное значение для логики
AI_HMI_str.PV := LREAL_TO_REAL(SEN_AI.Oper.PV_calc_eu);

// Обработанное значение для SCADA (Eu_user)
AI_HMI_str.PV_HMI := LREAL_TO_REAL(SEN_AI.Oper.PV_user);

IF NOT IMIT THEN
// Ошибка канала (обобщённая)
AI_HMI_str.Flt := SEN_AI.Oper.fault_code[00].0;

// Ошибка канала (КЗ)
AI_HMI_str.KZ := SEN_AI.Oper.fault_code[04].0;

// Ошибка канала (Обрыв)
AI_HMI_str.Obriv := SEN_AI.Oper.fault_code[05].0;

ELSE
AI_HMI_str.Flt 		:= FALSE;
AI_HMI_str.KZ 		:= FALSE;
AI_HMI_str.Obriv 	:= FALSE;
END_IF;


// Время, оставшееся до перевода канала в автомат
AI_HMI_str.TM_Repair_Back := SEN_AI.Oper.remaining_repair_time;

// Безударный переход Работа - ремонт
AI_HMI_str.Manual := SEN_AI.Config.Manual_Target;

// Единицы измерения для логики Eu_PV
AI_HMI_str.Eu_PV := SEN_AI.Config.Eu_PV;

// AS PS
AI_HMI_str.AS := (SEN_AI.Oper.PV_calc_eu >= AI_HMI_str.Ust.AS_H AND AI_HMI_str.Ust.AS_H_ctrl) OR (SEN_AI.Oper.PV_calc_eu <= AI_HMI_str.Ust.AS_L AND AI_HMI_str.Ust.AS_L_ctrl);
AI_HMI_str.PS := (SEN_AI.Oper.PV_calc_eu >= AI_HMI_str.Ust.PS_H AND AI_HMI_str.Ust.PS_H_ctrl) OR (SEN_AI.Oper.PV_calc_eu <= AI_HMI_str.Ust.PS_L AND AI_HMI_str.Ust.PS_L_ctrl);]]></ST>
        </FunctionBlock>
        <Function UUID="8D15F57049F5028192F2088F3E72CEF6" Name="AI_init" ResultTypeUUID="A32E304D43358DC1574ABBA8FDAFBC2D">
