                <Variable UUID="2055DDD5435182E309081FA6A061CECF" Name="min_ADC" Type="DINT" TypeUUID="AF9665534C6D5BDEF9AE77B026938B65" Usage="input" />
                <Variable UUID="91DB535A4BA9A454B6863C988A5C4DD4" Name="max_ADC" Type="DINT" TypeUUID="AF9665534C6D5BDEF9AE77B026938B65" Usage="input" />
                <Variable UUID="A13B68614D59FF3AF0E6C8A761E11F7B" Name="Eu_sensor" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="input" />
                <Variable UUID="E087522A4909BD7C48B172AAA8B8B9A1" Name="Eu_PV" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="input" />
                <Variable UUID="87E4772D45FC9CEAD17A5FAD205247E6" Name="Eu_user" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="input" />
                <Variable UUID="CF2C2E39497E9CD0D29EF8AB802E44AE" Name="Manual_Enable" Type="BYTE" TypeUUID="C3C10A82401AD741FA0AB89DDE8B5F30" Usage="input" />
                <Variable UUID="3D43BBF34F3DA111E5AFB18F1F9A49D4" Name="Manual_Auto" Type="BYTE" TypeUUID="C3C10A82401AD741FA0AB89DDE8B5F30" Usage="input" />
                <Variable UUID="46E9F64E489A651A13F21297DE53702A" Name="Manual_Target" Type="LREAL" TypeUUID="65F1DDD44EDA9C0776BB16BBDFE36B1F" Usage="input" />
                <Variable UUID="9CCFA974419BEA72636DCE9DEC768273" Name="HMI_INIT" Type="AI_HMI" TypeUUID="A66BBC2040DE5F1DB91157BC6ACE32E7" Usage="inout" />
                <Variable UUID="AB367F4B47FD3A1C43CDAF9E6C9AC8EB" Name="ROC" Type="LREAL" TypeUUID="65F1DDD44EDA9C0776BB16BBDFE36B1F" Usage="input" />
                <Variable UUID="439EEEA4444A700DD2AE198A0BDFD689" Name="Descr" Type="STRING" TypeUUID="38FDDE3B442D86554C56C884065F87B7" InitialValue="&apos;FALSE&apos;" Usage="input" />
            </Variables>
            <ST><![CDATA[AI_init.Config.Span:=Span;
AI_init.Config.Offset:=Offset;
AI_init.Config.gain := LREAL#0.0;
AI_init.Config.bias := LREAL#0.0;
AI_init.Config.ROC := ROC;	//Скорость нарастания сигнала
AI_init.Config.Tf:=Tf;
AI_init.Config.Manual_Target := Manual_Target;
AI_init.Config.Manual_Rate := Span/LREAL#20;
AI_init.Config.min_fault_sensor_Eu := Offset-Span*LREAL#0.025;
AI_init.Config.max_fault_sensor_Eu := Span+Offset+Span*LREAL#0.012;
AI_init.Config.recovery_time := LREAL#0.5;
AI_init.Config.repair_time := repair_time;
AI_init.Config.min_ADC := DINT_TO_LREAL(min_ADC);
AI_init.Config.max_ADC := DINT_TO_LREAL(max_ADC);
AI_init.Config.Eu_sensor := Eu_sensor;
AI_init.Config.Eu_PV := Eu_PV;
AI_init.Config.Eu_user := Eu_user;
AI_init.Config.TypePolynom := INT#0;
AI_init.Config.Manual_Enable := Manual_Enable;
AI_init.Config.Manual_Auto := Manual_Auto;
AI_init.Config.derivative_Enable := BYTE#0;
AI_init.Config.ROC_Enable := BYTE#0;
AI_init.Config.reset := BYTE#0;

HMI_INIT.Eu 		:= Eu_user;							//Eu  для SCADA
HMI_INIT.TM_Repair 	:= repair_time;						//Время вывода из ремонта
HMI_INIT.ROC 		:= ROC;								//Ошибка скорости измерения сигнала
HMI_INIT.Max 		:= LREAL_TO_REAL(Offset + Span);	//Максимум диапазона
HMI_INIT.Min 		:= LREAL_TO_REAL(Offset);			//Минимум диапазона
HMI_INIT.Descr		:= Descr;							//Название сигнала (Комментарий)]]></ST>
        </Function>
