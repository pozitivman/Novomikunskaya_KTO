                <Variable UUID="4929053F45575667DCDE2888863BDE83" Name="KR" Type="support_ACT" TypeUUID="E2F512E0418E53D1482E2F9E54A664A9" Usage="input" />
                <Variable UUID="594219CF45D54B44A4F8B9855BC373B9" Name="tmrCmdSP" Type="LREAL" TypeUUID="65F1DDD44EDA9C0776BB16BBDFE36B1F" Usage="input" />
                <Variable UUID="25C34A724E938D44D6FDDEBC7A4B2771" Name="tmrPressSP" Type="LREAL" TypeUUID="65F1DDD44EDA9C0776BB16BBDFE36B1F" Usage="input" />
                <Variable UUID="7384D9B042C08EA9AF4470B22897C89C" Name="cwOn" Type="BYTE" TypeUUID="C3C10A82401AD741FA0AB89DDE8B5F30" Usage="input" />
                <Variable UUID="477FED834A2CA702EAF08BBAD7FAB2D3" Name="cwOff" Type="BYTE" TypeUUID="C3C10A82401AD741FA0AB89DDE8B5F30" Usage="input" />
                <Variable UUID="51F6A11743F7F1308523CC85029AA033" Name="RA" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
