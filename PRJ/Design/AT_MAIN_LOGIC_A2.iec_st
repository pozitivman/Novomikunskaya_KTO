                <Variable UUID="6D122D7F4350A9D672209AB46B3F73BD" Name="APS_selfon" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="inout" />
                <Variable UUID="D1C0203940767BD7FC4A54A10BA41B68" Name="APS_selfoff" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="inout" />
                <Variable UUID="2CB7153642514A009DB0A784E8F88071" Name="APS_flt" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="inout" />
                <Variable UUID="5BAD2D2345EE67E29348829CA1149661" Name="APS_ncon" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="inout" />
                <Variable UUID="B2F0B32346920CED44EACCA891A4822F" Name="APS_ncoff" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="inout" />
                <Variable UUID="2F8C901842268CA512830BA22A0060E5" Name="APS_all" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="inout" />
                <Variable UUID="F0D106E64BB82DE2CDC7D3AA5A64C7BE" Name="KR_state" Type="WORD" TypeUUID="05CD1AAB4891B30C004C2799CA460884" Usage="inout" />
                <Variable UUID="3E3D13CA4AD7B407ADF3949EE4374D76" Name="APS_extern" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                <Variable UUID="A43987B6454F1D7386F50182C9D07289" Name="cmd_ON" Type="R_TRIG" TypeUUID="D90E3B974D4D8C72877123B1A897EB52" Usage="internal" />
                <Variable UUID="9E81B987458CCD3DEFBB0FAD0696C719" Name="cmd_OFF" Type="R_TRIG" TypeUUID="D90E3B974D4D8C72877123B1A897EB52" Usage="internal" />
                <Variable UUID="57F552494731483CEAC41394B178B526" Name="alg_ON" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
                <Variable UUID="A073A32C4CA560DCBAB5A891FEF7A3F7" Name="alg_OFF" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
            </Variables>
            <ST><![CDATA[KR.Control.RA.0 := HMI_KR_DA;
IF HMI_KR_rOn AND HMI_KR_DA  THEN alg_ON  := BOOL#1; END_IF; HMI_KR_rOn  := BOOL#0;
IF HMI_KR_rOff AND HMI_KR_DA THEN alg_OFF := BOOL#1; END_IF; HMI_KR_rOff := BOOL#0;
IF alg_ON AND CMD_KR_rOnOff OR alg_OFF AND NOT CMD_KR_rOnOff THEN
	KR_rep := BOOL#1;
ELSE
	IF alg_ON THEN CMD_KR_rOnOff := BOOL#1; END_IF;
	IF alg_OFF THEN CMD_KR_rOnOff := BOOL#0; END_IF;
END_IF;
alg_ON := BOOL#0;
alg_OFF := BOOL#0;

KR.Control.rOnOff.0 := CMD_KR_rOnOff;
KR.Control.aOnOff.0 := CMD_KR_aOnOff;
KR.Control.rep.0 := KR_rep; KR_rep := BOOL#0;
KR.Control.cwOn.0 := CMD_KR_cwOn;
KR.Control.cwOff.0 := CMD_KR_cwOff;


KR.Ins.on.0 := DI_KR_ON;
KR.Ins.off.0 := DI_KR_OFF;
KR.Ins.con.0 := DI_KRkcu_ON;
KR.Ins.coff.0 := DI_KRkcu_OFF;
APS_non := KR.Oper.non.0;
APS_noff := KR.Oper.noff.0;
APS_nstart := KR.Oper.nstart.0;
APS_selfon := KR.Oper.selfon.0;
APS_selfoff := KR.Oper.selfoff.0;
APS_flt := KR.Oper.flt.0;
APS_ncon := KR.Oper.ncon.0;
APS_ncoff := KR.Oper.ncoff.0;
KR_state.0 := KR.Ins.off.0;
KR_state.1 := KR.Ins.on.0;


//!!!!!!!!!!!Обобщенный ALARM !!!!!!!!!!!!!!!!!!!
if IMIT then APS_all := false; 
else
	APS_all := APS_non OR APS_noff OR APS_nstart OR APS_selfon OR APS_selfoff OR APS_flt OR APS_ncon OR APS_ncoff OR APS_extern;
end_if;
/////////////////////////////////////////////////


IF NOT HMI_KR_DA THEN CMD_KR_rOnOff := CMD_KR_aOnOff; ELSE CMD_KR_aOnOff := CMD_KR_rOnOff; END_IF;]]></ST>
        </FunctionBlock>
        <FunctionBlock UUID="A09AE2844B73351BF94B378381A47C0F" Name="VLV_imit">
            <Variables>
                <Variable UUID="E242F7EE45087E5F059C24B1D68412BD" Name="DO_KR_ON" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                <Variable UUID="6B2C68904FA05C90BF1AB18751794B74" Name="DO_KR_OFF" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                <Variable UUID="F61C4CE34333556EDF549AA9B05E6080" Name="DI_KR_ON" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="inout" />
                <Variable UUID="D999BD054A6D1605D6AE6B91A9A7C3D9" Name="DI_KR_OFF" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="inout" />
                <Variable UUID="D5390E814159D74B7F86C78309CB30F0" Name="delay_on" Type="TON" TypeUUID="EAA43B6B4DCDCEA72AB748B4554FD2AD" Usage="internal" />
                <Variable UUID="FD4C71AC4CC3AB663A2CE8AF53DA5E92" Name="delay_off" Type="TON" TypeUUID="EAA43B6B4DCDCEA72AB748B4554FD2AD" Usage="internal" />
                <Variable UUID="53A159A7404A9A0DBBC6F2B96D78FF12" Name="delay_vlv_bool1" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
                <Variable UUID="BBC283A34FACE69CB89212883D9AE416" Name="delay_vlv_bool2" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
                <Variable UUID="671D2129434643E69A11A19EEBBAB428" Name="delay_vlv_time1" Type="TIME" TypeUUID="3EFD49044E055B696F5570BA288FA8D5" Usage="internal" />
                <Variable UUID="1F2741F34ADEEAE92D36729DBFD13DA5" Name="delay_vlv_time2" Type="TIME" TypeUUID="3EFD49044E055B696F5570BA288FA8D5" Usage="internal" />
            </Variables>
            <ST><![CDATA[delay_on(DO_KR_ON, T#3s, delay_vlv_bool1, delay_vlv_time1);
IF delay_vlv_bool1  THEN DI_KR_ON := BOOL#1; DI_KR_OFF := BOOL#0; END_IF;
delay_off(DO_KR_OFF, T#3s, delay_vlv_bool2, delay_vlv_time2);
IF delay_vlv_bool2 THEN DI_KR_ON := BOOL#0; DI_KR_OFF := BOOL#1; END_IF;]]></ST>
        </FunctionBlock>
        <FunctionBlock UUID="F5D50D354EA4F055A7C45FA642B99021" Name="VLV_imit_11">
            <Variables>
                <Variable UUID="E242F7EE45087E5F059C24B1D68412BD" Name="DO_KR_ON" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                <Variable UUID="F61C4CE34333556EDF549AA9B05E6080" Name="DI_KR_ON" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="inout" />
                <Variable UUID="D5390E814159D74B7F86C78309CB30F0" Name="delay_on" Type="TON" TypeUUID="EAA43B6B4DCDCEA72AB748B4554FD2AD" Usage="internal" />
                <Variable UUID="FD4C71AC4CC3AB663A2CE8AF53DA5E92" Name="delay_off" Type="TON" TypeUUID="EAA43B6B4DCDCEA72AB748B4554FD2AD" Usage="internal" />
                <Variable UUID="0BE067CA43DE204CA61943AF22EEDB0E" Name="rtrig" Type="R_TRIG" TypeUUID="D90E3B974D4D8C72877123B1A897EB52" Usage="internal" />
                <Variable UUID="844E2E10406FA1ABC83E3EAFBEB36049" Name="ftrig" Type="F_TRIG" TypeUUID="F64DAE304009E1613B4AB4AF27532AEF" Usage="internal" />
            </Variables>
            <ST><![CDATA[rtrig(CLK:= DO_KR_ON);
delay_on(IN:= rtrig.Q, PT:= T#3s);
if delay_on.Q then DI_KR_ON := true; end_if;

ftrig(CLK:= DO_KR_ON);
delay_off(IN:= ftrig.Q, PT:= T#3s);
if delay_off.Q then DI_KR_ON := false; end_if; ]]></ST>
        </FunctionBlock>
        <Function UUID="16FC7ADE4362B2E807A59DA0715A2A75" Name="VLV_init" ResultTypeUUID="E2F512E0418E53D1482E2F9E54A664A9">
