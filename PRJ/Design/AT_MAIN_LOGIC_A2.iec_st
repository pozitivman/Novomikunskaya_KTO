                <Variable UUID="8E2AE4714002E8F2AEAC5283280EE478" Name="HMI_rOff" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                <Variable UUID="73F1D5D74FC303C22CE3C29F5A8151B9" Name="LG_state" Type="WORD" TypeUUID="05CD1AAB4891B30C004C2799CA460884" Usage="input" />
                <Variable UUID="B4241ADC46CEED6CB48E978AEACCD611" Name="LG_flt_all" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                <Variable UUID="7BBEEA7D44FD2737525B40A87A86A8AF" Name="PRU_state" Type="WORD" TypeUUID="05CD1AAB4891B30C004C2799CA460884" Usage="inout" />
                <Variable UUID="864DE8364A8C9FE134B595B047138C37" Name="HMI_state" Type="WORD" TypeUUID="05CD1AAB4891B30C004C2799CA460884" Usage="inout" />
                <Variable UUID="BC1E72B44C026D240F32D2818F430749" Name="PRU_flt_all" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="inout" />
                <Variable UUID="072059D64441AD36402E2C8C25296063" Name="HMI_flt_all" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="inout" />
                <Variable UUID="2B5188684FACDC33646582A4BE7C8058" Name="DA" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="inout" />
                <Variable UUID="0240C45844E18D10DA6B64A6E90A10A1" Name="DA_PRU" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="inout" />
                <Variable UUID="35ABCCB44FA5E76BE05FF884CB8BCB36" Name="DA_HMI" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="inout" />
                <Variable UUID="77225DF145E844B392BAA0ABA49A8202" Name="rOn" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="inout" />
                <Variable UUID="CA50BEC9482710C5792FB98ABD3BBD0C" Name="rOff" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="inout" />
            </Variables>
            <ST><![CDATA[//перевод ИМ в дистанционный/автоматический режим
if PRU_DA_on or HMI_DA_on then DA := BOOL#1; end_if;
if PRU_DA_off or HMI_DA_off then DA := BOOL#0; end_if;

//Передаем руч/дист ИМ из блока в разные приемники
if DA then
	DA_PRU := true;
	DA_HMI := true;
else
	DA_PRU := false;
	DA_HMI := false;
end_if;

//Передаем обобщенную аварию ИМ из блока в разные приемники
if LG_flt_all then
	PRU_flt_all := true;
	HMI_flt_all := true;
else
	PRU_flt_all := false;
	HMI_flt_all := false;
end_if;

//Передаем состояние ИМ из блока в разные приемники
PRU_state := LG_state;
HMI_state := LG_state;
	
//Дистанционное управление ИМ
if PRU_rOn OR HMI_rOn then rOn := BOOL#1; end_if;
if PRU_rOff OR HMI_rOff then rOff := BOOL#1; end_if;]]></ST>
        </FunctionBlock>
        <FunctionBlock UUID="90D0C5D044939603D0BD418B90A10328" Name="DI_Proc">
            <Variables>
                <Variable UUID="293790C0400AEE8E646241AF54168597" Name="DI_raw" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                <Variable UUID="90D020944A62F4359AC073BD30D91C04" Name="cycle_set" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="input" />
                <Variable UUID="6DA04EE34C5683D9CA5E5DA173F1E282" Name="Inv" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                <Variable UUID="9EBB87F74FDE47608E622599DCEFC25E" Name="DI_out" Type="DI_HMI" TypeUUID="24096E3F4C0C396B81199DA606D417FF" Usage="inout" />
                <Variable UUID="9238B59F4BC1A60E4E950DBC947C2480" Name="Descr" Type="STRING" TypeUUID="38FDDE3B442D86554C56C884065F87B7" InitialValue="&apos;FALSE&apos;" Usage="input" />
                <Variable UUID="74FB70564CC19DC7C703E6B32B39C126" Name="cycle_val" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                <Variable UUID="AFC9DC1340331290F03520A18FE89153" Name="DI_in_last" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="internal" />
            </Variables>
            <ST><![CDATA[IF NOT (DI_Raw XOR DI_in_last) THEN
	cycle_val := cycle_val + INT#1;
ELSE
	cycle_val := INT#0;
END_IF;

IF cycle_val >= cycle_set THEN
	DI_out.PV := DI_Raw XOR Inv;
END_IF;

DI_in_last := DI_Raw;
DI_out.Descr := Descr;]]></ST>
        </FunctionBlock>
